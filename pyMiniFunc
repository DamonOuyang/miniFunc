#################################^^^#########^^^##
##            12. 深拷贝 和 浅拷贝              ##
######################################~..~########
# !/usr/bin/python  
# coding:utf-8  
# -*- coding:utf-8 -*-
'''
list1 = [1,2]
list2 = list1 # 就是个引用，你操作list2 , 其实list1的结果也会变

list1.append(55)
list3 = list1[:]
print(list1)
# print(list2)
# print(list3)

import copy
list4 = copy.copy(list1) # 他和list3一样 都是浅拷贝
# print(list4)
# print(id(list1))
# print(id(list2))
# print(id(list3))
# print(id(list4))

list2[0] = 3
print("list1:", list1)

list3[0] = 4
list4[1] = 4
print("list1:",list1) # 对list3 和 list4操作都没有对 list1 有影响
'''
# 再看看深拷贝和浅拷贝的区别
from copy import copy, deepcopy
list1 = [[1], [2]]
list2 = copy(list1) # 还是浅拷贝
print(id(list1), id(list2))
# 39872032 39871552     
list3 = deepcopy(list1) # 深拷贝
print(id(list1), id(list2), id(list3))
# 39872032 39871552 39873592 # 深拷贝
print("list1:", list1)
list2[0][0] = 3
print("list1:", list1) # 看到了吧，假如你操作其子对象  还是和引用一样 影响了源
print("list2:", list2) # 原来的也被修改了，浅拷贝
# list1: [[1], [2]]

# list1: [[3], [2]]
# list2: [[3], [2]]

list3[0][0] = 5
print("list1:", list1)
print("list3:", list3)
# list1: [[3], [2]]
# list3: [[5], [2]]

