
#############################
# c 
# 移位实现权限管理
#############################
#include <stdio.h>
int main()
{
	int num = 588;
	int j = 0;
	int arr[11] = {0};
	int ii=0;
	for(ii = 0; ii < 11; ii++)	
	{
		if((num>>ii)&1)
		{
			arr[j++] = ii;
		}
	}
	
	int i = 0;
	for(;i<10;i++)
	{
		printf("%d\n",arr[i]);
	}
	return 0;
}
